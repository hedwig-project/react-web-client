{
  "name": "hedwig_web",
  "version": "0.0.1",
  "description": "Web client for Hedwig project",
  "main": "index.js",
  "engines": {
    "node": "~7.7.1",
    "npm": "~4.2.0"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=development webpack-dev-server -d --progress --config ./webpack.dev.config.js --port 3000 --host 0.0.0.0 --history-api-fallback",
    "build": "cross-env NODE_ENV=production yarn run clean && webpack --progress --config ./webpack.prod.config.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "mocha --reporter spec --require babel-core/register",
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register",
    "codecov": "codecov -t af290123-29c6-4359-b9fc-a63b0bddfcef",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": "https://github.com/hedwig-project/react-web-client",
  "author": "Daniela Yassuda, Gabriela Melo, Hugo Possani, Victor Takashi",
  "license": "MIT",
  "dependencies": {
    "immutable": "~3.8.1",
    "joi-full": "^10.0.6-rel3",
    "lodash": "^4.17.4",
    "material-ui": "0.16.7",
    "object-path": "^0.11.3",
    "react": "15.4.1",
    "react-dom": "15.4.0",
    "react-icons": "^2.2.3",
    "react-md-spinner": "^0.1.0",
    "react-redux": "~4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "~4.0.6",
    "react-tap-event-plugin": "2.0.1",
    "redux": "~3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-form": "^6.4.3",
    "redux-form-material-ui": "^4.1.2",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "redux-responsive": "^4.1.1",
    "styled-components": "^1.4.6",
    "webpack": "^2.2.0-rc.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "codecov": "^1.0.1",
    "compression-webpack-plugin": "^0.3.2",
    "coveralls": "^2.11.16",
    "cross-env": "^4.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "getstorybook": "^1.7.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
